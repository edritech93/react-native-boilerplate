{
  "name": "boilerplateRN",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator \"iPhone 8\"",
    "ipad": "react-native run-ios --simulator \"iPad Air (4th generation)\"",
    "start": "react-native start --reset-cache",
    "test": "jest",
